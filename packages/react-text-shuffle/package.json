{
  "name": "@jnsdls/react-text-shuffle",
  "version": "0.1.0",
  "private": false,
  "license": "MIT",
  "source": "src/index.ts",
  "main": "dist/react-text-shuffle.js",
  "exports": "./dist/react-text-shuffle.modern.js",
  "module": "dist/react-text-shuffle.module.js",
  "unpkg": "dist/react-text-shuffle.umd.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build:pkg": "microbundle --jsx React.createElement",
    "build:move-types": "mv ./dist/react-text-shuffle/src/* ./dist/ && rm -rf ./dist/react-text-shuffle",
    "build": "yarn build:pkg && yarn build:move-types",
    "dev": "microbundle watch",
    "clean": "rm -rf dist/ && rm -rf node_modules/",
    "lint:src": "eslint src --ext .ts,.tsx --config ../../.eslintrc",
    "lint:types": "tsc --noEmit"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@jnsdls/utils": "^0.1.0",
    "calculate-size": "^1.1.1"
  },
  "peerDependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "framer-motion": "^3.10.0"
  },
  "devDependencies": {
    "react-dom": "^17.0.1",
    "framer-motion": "^3.10.0",
    "react": "^17.0.1",
    "microbundle": "^0.13.0",
    "typescript": "^4.2.3",
    "@types/react": "^17.0.3",
    "lint-staged": "^10.0.10"
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "npm run lint",
      "npm run format"
    ]
  }
}
